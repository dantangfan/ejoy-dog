{
    "manifest_version": 2,
    "name": "EJOY-DOG",
    "version": "1.0",
    "description": "EJOY 二次认证",
    "content_scripts": [
        {
            "matches": ["*://one.ejoy.com/login*", "*://testone.ejoy.com/login*", "*://one.ejoy.com/oauth*", "*://testone.ejoy.com/oauth*"],
            "js": ["jquery.min.js", "dog.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "clipboardWrite",
        "clipboardRead"
    ]

}
